  "private": true,
  "description": "Blokino - Web environment to program physical objects.",
  "main": "public/electron.js",
  "homepage": "./",
  "email": "jorge.farfan1990@gmail.com",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@mui/styles": "^5.14.16",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "avrgirl-arduino": "^5.0.1",
    "blockly": "^10.2.2",
    "bootstrap": "^4.5.2",
    "chalk": "^2.4.2",
    "code-mirror-themes": "^1.0.0",
    "codemirror": "^5.57.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^3.3.0",
    "electron-is-dev": "^2.0.0",
    "esprima": "^4.0.1",
    "husky": "^4.3.8",
    "i18next": "^23.6.0",
    "internet-available": "^1.0.0",
    "johnny-five": "^2.1.0",
    "js-beautify": "^1.13.0",
    "json-cycle": "^1.3.0",
    "macos-release": "^3.0.1",
    "moment": "^2.27.0",
    "node-pty": "^0.10.0-beta16",
    "oled-js": "^5.1.2",
    "os": "^0.1.1",
    "os-name": "^3.1.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.10.1",
    "react-i18next": "^11.18.6",
    "react-icons": "^4.2.0",
    "react-intl": "^5.12.3",
    "react-redux": "^7.2.9",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
