  "devDependencies": {
    "concurrently": "^6.4.0",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.9",
    "wait-on": "^6.0.0"
  },
  "license": "MIT",
  "author": "Jorge Farfan Coaguila<jorge.farfan1990@gmail.com>",
  "keywords": [
    "Blokino",
    "Electron",
    "JavaScript",
    "Johnny-Five"
  ],
  "build": {
    "appId": "Blokino-v0.0.4",
    "extends": null, 
    "asar": false,
    "files": [
      "./build/**/*",
      "package.json",
      "node_modules",
      "*.js",
      "build/static/css/*.css",
      "build/static/css/*.css.map",
      "build/static/js/*.js",
      "build/static/js/*chunk.js",
      "build/static/js/*map.js",
      "build/static/media/*.svg",
      "node_modules",
      "./public/**/*",
      "./src/**/*"
    ],
    "directories": {
      "buildResources": "public/assets/**"
    },
    "productName": "Blokino",
    "mac": {
      "category": "organizer",
      "icon": "public/icons/mac/icon.icns"
    },
    "dmg": {},
    "linux": {
      "target": [
        "deb"
      ],
      "files": [
        "!src/resources/circuits/diagrams/*",
        "!src/resources/examples-code/*",
        "!src/resources/installers/*",
        "!src/resources/notes/*",
        "!src/resources/README.md"
      ]
    }
  },
  "scripts": {
    "start-electron": "electron . --no-sandbox",
    "start": "react-scripts start",
    "electron-rebuild": "electron-rebuild",
    "react-build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron --disable-gpu .\"",
    "star-app": "concurrently \"BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron --no-sandbox --disable-gpu .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "installer": "react-scripts build  && electron-builder --dir"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
